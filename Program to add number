#include <stdio.h>
int main() {    

    int number1, number2, sum;//variables
    
    printf("Enter two integers: ");
    scanf("%d %d", &number1, &number2);//taking inputs from user

    // calculating sum
    sum = number1 + number2;      
    
    printf("%d + %d = %d", number1, number2, sum); //printing the sum result
    return 0;
}
